# Keybindings
killall sxhkd; sxhkd &

# Wallpaper
nitrogen --restore &

# Notifications
dbus-launch dunst --config ~/.config/dunst/dunstrc

# Polkit
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &

# Compositor
picom -c &

# Dex
dex -a -s /etc/xdg/autostart/:~/.config/autostart &

# Polybar
polybar example &

# NM-Applet
nm-applet &
